doctype html
html(xmlns:ng="http://angularjs.org" ng-app="unity-profiles-app")
    head
        title #{title} - Unity Profiles 
        link(rel='stylesheet' href='css/gumby.css')
        script(
            type='text/javascript',
            src='js/libs/modernizr-2.6.2.min.js'
            )
    body
        #navMain.row.navbar.pretty
            a(class='toggle' gumby-trigger='#navMain > ul' href='#')
                i.icon-menu
            h1.four.columns.logo
                a(href='#')
                    img(src='/img/gumby_mainlogo.png' gumby-retina)

            ul.eight.columns
                li
                    a(href='#') Projects
                        .dropdown
                            ul
                                li 
                                    a(href='#') List item 1 
                                    a(href='#') List item 2
                                    a(href='#') List item 3 
                                    a(href='#') List item 4 
                li
                    a(href='#') Start Session

                li 
                    a Login Details

        
        block content

        footer
            br
            br
            br

        include ./javascript.jade
